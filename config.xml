<?xml version='1.0' encoding='utf-8'?>
<widget id="com.phonegap.binghamtonphilharmonic" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Binghamton Philharmonic</name>
    <description>
        A educational app designed with Framework7, Vue, and Phonegap for the Binghamton Philharmonic.
    </description>
    <author email="contact@huestudios.com" href="http://huestudios.com">
        Hue
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
        <preference name="BackupWebStorage" value="none" />
    </platform>
    <preference name="DisallowOverscroll" value="true" />
    <engine name="ios" spec="~4.4.0" />
    <plugin name="cordova-plugin-console" spec="~1.0.1" />
    <plugin name="cordova-plugin-statusbar" spec="~1.0.1" />
    <plugin name="cordova-plugin-whitelist" spec="~1.2.0" />
</widget>
